{
  "version": 2,
  "functions": {
    "api/main_with_auth.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "api/main_with_auth.py"
    },
    {
      "src": "/auth/(.*)",
      "dest": "api/main_with_auth.py"
    },
    {
      "src": "/subscription/(.*)",
      "dest": "api/main_with_auth.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "api/main_with_auth.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/main_with_auth.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main_with_auth.py"
    }
  ],
  "env": {
    "PYTHONPATH": "api"
  }
}